{
  "name": "party-server",
  "version": "0.1.0",
  "description": "A full stack web app for parties",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "clean": "rm -rf dist",
    "test": "tape -r ts-node/register src/**/*.spec.ts | faucet",
    "test:coverage": "nyc npm test",
    "test:watch": "sane 'nyc npm test' --glob='**/*.spec.ts' --wait=2",
    "lint": "tslint -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chances/party-server.git"
  },
  "keywords": [
    "party",
    "full-stack",
    "typescript",
    "nest"
  ],
  "author": "Chance Snow <git@chancesnow.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chances/party-server/issues"
  },
  "homepage": "https://github.com/chances/party-server#readme",
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "dotenv": "^5.0.1",
    "lit-ntml": "^0.4.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.8",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^4.6.6",
    "@types/chai": "^4.1.2",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/node": "^9.6.0",
    "@types/tape": "^4.2.32",
    "chai": "^4.1.2",
    "faucet": "0.0.1",
    "nodemon": "^1.17.2",
    "nyc": "^11.6.0",
    "sane": "^2.5.0",
    "tape": "^4.9.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.1.3",
    "tslint": "^5.9.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.spec.ts",
      "src/test/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "lcov",
      "html"
    ],
    "all": true
  }
}
